{
  "name": "post",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "votesObtained": {
      "type": "number",
      "default": 0,
      "required": true
    },
    "tags": {
      "type": [
        "string"
      ],
      "required": true
    },
    "date": {
      "type": "date",
      "defaultFn": "now"
    },
    "attachment": {
      "type": "any"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "postId",
      "options": {
        "nestRemoting": true
      }
    },
    "votes": {
      "type": "hasMany",
      "model": "vote",
      "foreignKey": "",
      "options": {
        "nestRemoting": true
      }
    }
  },
  "acls": [],
  "methods": {}
}
